{
  "expo": {
    "name": "IBeatRewritten",
    "slug": "IBeatRewritten",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "IBRewritten",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.IBeatRewritten"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.IBeatRewritten",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "eas": {
      "projectId": "842f33b8-475f-4a88-9bc6-d1c0c822d4f8"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "842f33b8-475f-4a88-9bc6-d1c0c822d4f8"
      }
    }
  }
}
